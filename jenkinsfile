pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git branch: 'develop', url: 'https://github.com/DevarajVin/Vin_Intallipaat.git'
            }
        }
        stage('Pull Content') {
            steps {
                sh 'mkdir main'
                sh 'cp -r * main'
            }
        }
    }
}

